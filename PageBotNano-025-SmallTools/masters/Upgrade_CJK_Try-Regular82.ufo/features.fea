#
# --- Bitcount-Regular Features ---
#

@g = [ g gcircumflex gbreve gdotaccent gcommaaccent gcaron ];
@g_nct = [ g.nct gcircumflex.nct gbreve.nct gdotaccent.nct gcommaaccent.nct gcaron.nct ];
